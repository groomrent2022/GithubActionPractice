name: Second Workflow

on:
  workflow_run:
    workflows: ["Trigger Workflow"]
    types:
      - completed

jobs:
  process_input:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout repository
        uses: actions/checkout@v2

      - name: Print user name and message
        run: |
          echo "Received user name: ${{ github.event.inputs.user_name }}"
